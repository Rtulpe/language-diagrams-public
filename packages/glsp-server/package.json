{
  "name": "language-diagrams-server",
  "version": "0.0.1",
  "description": "GLSP node server for language diagrams",
  "repository": {
    "type": "git"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@eclipse-glsp/layout-elk": "^1.0.0",
    "@eclipse-glsp/protocol": "^1.0.0",
    "@eclipse-glsp/server-node": "^1.0.0",
