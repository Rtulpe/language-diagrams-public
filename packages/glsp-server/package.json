    "language-diagrams-protocol": "0.0.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "~20.11.25",
    "circular-dependency-plugin": "^5.2.2",
    "depcheck": "1.4.3",
    "eslint": "^8.47.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.6.2",
    "jest-expect-message": "^1.1.3",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.1",
    "rimraf": "latest",
    "source-map-loader": "^5.0.0",
    "ts-jest": "^29.1.1",
